<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary">Data Items</h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>#</th>
            <th>Invoice</th>
            <th>Date</th>
            <th>Item</th>
            <th>Member Name</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tfoot>
          <tr>
            <th>#</th>
            <th>Invoice</th>
            <th>Date</th>
            <th>Item</th>
            <th>Member Name</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </tfoot>
        <tbody>
          <% for(let i = 0; i < booking.length; i++) { %>
          <tr>
            <td><%= i + 1; %> </td>
            <td>#<%= booking[i].invoice %> </td>
            <td><%= booking[i].bookingStartDate.toLocaleDateString('id-ID') %> s/d
              <%= booking[i].bookingEndDate.toLocaleDateString().split(',')[0] %></td>
            <td><%= booking[i].itemId.title %> </td>
            <td><%= booking[i].memberId.firstName %> <%= booking[i].memberId.lastName %> </td>
            <td> <% if(booking[i].payments.status == 'Proses') { %>
              <span class="badge badge-primary"><i class="fas fa-spinner"></i> <%= booking[i].payments.status %> </span>
              <% }else if(booking[i].payments.status == 'Accept') { %>
              <span class="badge badge-success"><i class="fas fa-check"></i> <%= booking[i].payments.status %> </span>
              <%} else {%>
              <span class="badge badge-danger"><i class="fas fa-times"></i> <%= booking[i].payments.status %> </span>
              <%} %>
            </td>
            <td>
              <a href="/admin/booking/<%= booking[i].id %> " class="btn btn-success btn-circle btn-sm">
                <i class="fas fa-eye"></i></a>
            </td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>